{
    "name": "paulmillband/cached-image-resizer",
    "description": "An automated way to resize images on demand, for frontend use. Primarily for use in Laravel vue.",
    "license": "MIT",
    "keywords": [
        "laravel8",
        "module",
        "image",
        "resize"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Paul Millband",
            "homepage": "https://github.com/yorick2",
            "role": "Developer"
        }
    ],
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.com/donate/?hosted_button_id=95TTM4Z9Q7MNG"
        }
    ],
    "autoload": {
        "psr-4": {
            "paulmillband\\cachedImageResizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "paulmillband\\cachedImageResizer\\ImageResizerPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
